{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "pwa-chrome",
        "request": "launch",
        "name": "Launch Chrome against localhost with /heyportal",
        "url": "http://localhost:3000/heyportal",
        "webRoot": "${workspaceFolder}/src",
        "sourceMaps": true,
        "breakOnLoad": true,
        "skipFiles": [
          "node_modules/**"
        ]
      }
    ]
  }
  